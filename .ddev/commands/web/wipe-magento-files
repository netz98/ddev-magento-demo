#!/bin/bash

## Description: Wipe Magento files in demo
## Usage: wipe-magento-files
## Example: "wipe-magento-files"
## ProjectTypes: magento2

cd /var/www/html;

rm -Rf \
    .editorconfig \
    .gitignore \
    .htaccess \
    .htaccess.sample \
    .php_cs.dist \
    .user.ini \
    app \
    auth.json.sample \
    bin \
    CHANGELOG.md \
    composer.json \
    composer.lock \
    COPYING.txt \
    dev \
    generated \
    grunt-config.json.sample \
    Gruntfile.js.sample \
    lib \
    magento \
    LICENSE.txt \
    LICENSE_AFL.txt \
    nginx.conf.sample \
    package.json.sample \
    phpserver \
    pub \
    SECURITY.md \
    setup \
    var \
    vendor \
    ;
 
